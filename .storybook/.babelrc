{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": false,
        "shippedProposals": true,
        "targets": {
          "ie": "11",
          "edge": "16",
          "firefox": "85",
          "chrome": "86",
          "safari": "12",
          "node": "12.16.3"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "inline-react-svg",
      {
        "svgo": {
          "plugins": [
            {
              "removeViewBox": false,
              "cleanupIDs": false
            }
          ]
        }
      }
    ],
    [
      "babel-plugin-styled-components",
      {
        "displayName": true,
        "fileName": true
      }
    ]
  ],
  "exclude": ["node_modules"]
}
