{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "ie": 11,
          "edge": 14,
          "firefox": 45,
          "chrome": 49,
          "safari": 10,
          "node": "6.11"
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "inline-svg",
      {
        "svgo": {
          "plugins": [
            {
              "removeViewBox": false
            }
          ]
        }
      }
    ],
    "@quickbaseoss/babel-plugin-styled-components-css-namespace",
    [
      "babel-plugin-styled-components",
      {
        "displayName": true,
        "fileName": true
      }
    ]
  ],
  "exclude": ["node_modules"]
}
