  "name": "@helpscout/hsds-react",
  "version": "3.1.2",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "license": "MIT",
  "description": "React component library for Help Scout's Design System",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpscout/hsds-react.git"
  },
  "author": "Jon Quach <hello@jonquach.com>",
