{
  "name": "@helpscout/blue",
  "version": "1.7.1",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "license": "MIT",
  "description": "Help Scout's React component library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpscout/blue.git"
  },
  "authors": [
    "ItsJonQ <hello@jonquach.com>"
  ],
  "scripts": {
    "check:npm": "node scripts/check-npm.js",
    "prestart": "npm run check:npm",
    "build-storybook": "build-storybook",
    "build:css": "node scripts/sass.js",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --ignore *.test.js,tests --out-dir ./dist",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./dist/index.es.js",
    "build:es2015modulesSCSS": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/styles/includePaths.js --out-file ./dist/styles/includePaths.es.js",
    "build:copy-files": "babel-node ./scripts/copy-files",
    "build:js": "npm run build:es2015 && npm run build:es2015modules && npm run build:es2015modulesSCSS && npm run build:copy-files",
    "build": "npm run clean && npm run copy:css && npm run build:css && npm run build:js",
    "copy:css": "cpx 'src/styles/**/*.scss' 'dist/styles'",
    "coverage": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls",
    "clean": "rimraf public && rimraf dist && mkdir dist",
    "dev": "npm run check:npm && npm run test:dev",
    "deploy": "npm run storybook:clean && npm run storybook:build && npm run surge:cname && npm run surge:deploy",
    "karma": "BABEL_ENV=development NODE_ENV=test karma start config/karma.conf.js",
    "lint": "npm run lint:es",
    "lint:es": "eslint src/**/*.js --cache",
    "lint:css": "stylelint 'src/styles/**/*.scss'",
    "pretty": "npm run prettier",
    "release": "np --no-yarn --no-publish",
    "release:patch": "npm run release -- patch",
    "release:beta": "npm run release -- prerelease --yolo --any-branch",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 8900",
    "storybook:build": "NODE_ENV=production build-storybook -c .storybook -o public",
    "storybook:clean": "rimraf public",
    "surge:cname": "echo 'hsblue.surge.sh' > public/CNAME",
    "surge:deploy": "surge ./public",
    "test": "npm run test:coverage",
    "test:acceptance": "npm run karma -- --single-run",
    "test:all": "npm run test && npm run test:acceptance",
    "test:coverage": "npm run test:dev -- --coverage",
    "test:dev": "node scripts/test.js --env=jsdom",
    "test:coverage:fast": "npm run test:coverage -- --onlyChanged",
    "version": "npm run build",
    "postversion": "npm publish dist",
    "prettier:base": "prettier \"src/**/*.js\" \"test/**/*.js\" \"stories/**/*.js\" --parser=flow",
    "prettier": "npm run prettier:base -- --write"
  },
  "peerDependencies": {
    "react": "^15",
    "react-dom": "^15"
  },
  "dependencies": {
    "@helpscout/fancy": "^2.0.5",
    "@helpscout/react-utils": "^1.0.1",
    "closest": "0.0.1",
    "emoji-mart": "^1.0.1",
    "highlight.js": "^9.12.0",
    "lodash.get": "^4.4.2",
    "pluralize": "^7.0.0",
    "popper.js": "1.14.3",
    "prop-types": "15.6.1",
    "react-router-dom": "^4",
    "react-sortable-hoc": "^0.6.7",
    "react-transition-group": "2.2.0",
    "seed-alert": "0.2.6",
    "seed-button": "0.3.0",
    "seed-color-scheme": "0.3.0",
    "seed-control": "0.1.0",
    "seed-dash": "0.0.2",
    "seed-family": "0.0.4",
    "seed-grid": "0.4.0",
    "seed-list": "0.0.17",
    "seed-this": "0.0.5"
  },
  "devDependencies": {
    "@helpscout/helix": "^0.0.13",
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-options": "^3.2.16",
    "@storybook/cli": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "@storybook/storybook-deployer": "2.0.0",
    "autoprefixer": "7.1.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-inline-svg": "0.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "3.1.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chai": "4.1.2",
    "chalk": "1.1.3",
    "check-dependencies": "1.1.0",
    "coveralls": "3.0.1",
    "cpx": "1.5.0",
    "cross-env": "5.1.0",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "enzyme": "2.9.1",
    "eslint": "4.19.1",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.9.1",
    "eslint-scope": "3.7.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "flow-bin": "0.72.0",
    "fs-extra": "3.0.1",
    "hoek": "5.0.3",
    "html-webpack-plugin": "2.29.0",
    "husky": "1.0.0-rc.13",
    "istanbul-api": "1.2.2",
    "istanbul-reports": "1.1.4",
    "jasmine": "2.8.0",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "jquery": "3.2.1",
    "karma": "2.0.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-matchers": "3.7.0",
    "karma-webpack": "3.0.0",
    "lodash": "4.17.10",
    "mkdirp": "0.5.1",
    "node-sass": "4.9.2",
    "np": "^3.0.4",
    "nyc": "12.0.2",
    "object-assign": "4.1.1",
    "object-values": "2.0.0",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "postcss-scss": "1.0.2",
    "prettier": "1.12.1",
    "pretty-quick": "^1.4.1",
    "raw-loader": "0.5.1",
    "react": "15.6.1",
    "react-addons-test-utils": "15.6.2",
    "react-dev-utils": "^3.0.2",
    "react-dom": "15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-frame-component": "^1.1.0",
    "react-page-object": "1.1.0",
    "react-router": "4.2.0",
    "react-test-renderer": "15.6.1",
    "rgb-hex": "2.1.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "sass-pathfinder": "0.0.5",
    "seed-harvester": "0.1.5",
    "sinon": "^5.0.10",
    "style-loader": "0.18.2",
    "stylelint": "7.9.0",
    "stylelint-scss": "1.5.1",
    "surge": "0.20.1",
    "tough-cookie": "2.3.4",
    "url-loader": "1.0.1",
    "webpack": "2.6.1",
    "webpack-dev-server": "^3.1.8",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-node-externals": "1.6.0"
  },
  "nyc": {
    "exclude": [
      "config/**/*.js",
      "dist/**/*.js",
      "scripts/**/*.js",
      "stories/**/*.js"
    ],
    "cache": false
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/?(*.)types.{js,jsx}",
      "!src/**/?(*.)helpers.{js,jsx}",
      "!src/**/?(*.)testHelpers.{js,jsx}",
      "!src/styles/tests/helpers/**/*.{js,jsx}",
      "!src/**/?(*.)css.{js,jsx}",
      "!src/index.{js,jsx}",
      "!src/components/Input/utils.{js,jsx}",
      "!src/components/Popper/**/*.{js,jsx}",
      "!src/tests/helpers/**/*.{js,jsx}",
      "!src/utilities/browser.{js,jsx}",
      "!src/utilities/smoothScroll.{js,jsx}",
      "!src/utilities/index.{js,jsx}",
      "!src/utilities/log.{js,jsx}",
      "!src/styles/includePaths.js",
      "!src/polyfills/**/*.{js,jsx}",
      "!src/vendors/**/*.{js,jsx}",
      "!src/components/index.js",
      "!src/components/Animate/animations/**/*.js"
    ],
    "coverageDirectory": "coverage/",
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jsdomPolyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "rules": {
      "indentation": [
        2
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
