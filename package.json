{
  "name": "@helpscout/hsds-react",
  "version": "2.1.0",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "license": "MIT",
  "description": "React component library for Help Scout's Design System",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpscout/hsds-react.git"
  },
  "author": "Jon Quach <hello@jonquach.com>",
  "contributors": [
    "Nickolas Kenyeres <nickolas@knicklabs.com>",
    "Maxi Ferreira <charca@gmail.com>",
    "Tom Graham",
    "Tom O'Connor",
    "Brett Jones <brettjonesdev@gmail.com>",
    "Lauren Brown",
    "Pierre-Luc Babin",
    "Buzz Usborne <hello@buzzusborne.com>",
    "Alisdair McDiarmid"
  ],
  "scripts": {
    "check:npm": "node scripts/check-npm.js",
    "prestart": "npm run check:npm",
    "build-storybook": "build-storybook",
    "build:css": "node scripts/sass.js",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --ignore *.test.js,tests --out-dir ./dist",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./dist/index.es.js",
    "build:es2015modulesSCSS": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/styles/includePaths.js --out-file ./dist/styles/includePaths.es.js",
    "build:copy-files": "babel-node ./scripts/copy-files",
    "build:js": "npm run build:es2015 && npm run build:es2015modules && npm run build:es2015modulesSCSS && npm run build:ts && npm run build:copy-files",
    "build:ts": "tsc",
    "build": "npm run clean && npm run copy:css && npm run build:css && npm run build:js",
    "copy:css": "cpx 'src/styles/**/*.scss' 'dist/styles'",
    "coverage": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls",
    "clean": "rimraf public && rimraf dist && mkdir dist",
    "dev": "npm run check:npm && npm run test:dev",
    "jest": "node scripts/test.js --env=jsdom",
    "lint": "npm run lint:js",
    "lint:js": "eslint src/**/*.js --cache",
    "pretty": "npm run prettier",
    "release": "np --no-yarn --no-publish",
    "release:patch": "npm run release -- patch",
    "release:beta": "npm run release -- prerelease --yolo --any-branch",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 8900",
    "storybook:build": "NODE_ENV=production build-storybook -c .storybook -o public",
    "storybook:clean": "rimraf public",
    "test": "npm run test:coverage",
    "test:all": "npm run test",
    "test:coverage": "npm run test:dev -- --coverage",
    "test:dev": "node scripts/test.js --env=jsdom",
    "test:coverage:fast": "npm run test:coverage -- --onlyChanged",
    "version": "npm run build",
    "postversion": "npm publish dist",
    "prettier:base": "prettier \"src/**/*.js\" \"test/**/*.js\" \"stories/**/*.js\" --parser=flow",
    "prettier": "npm run prettier:base -- --write",
    "update:icons": "node ./scripts/update-icons.js"
  },
  "peerDependencies": {
    "react": "^15",
    "react-dom": "^15"
  },
  "dependencies": {
    "@helpscout/fancy": "^2.1.2",
    "@helpscout/react-utils": "^1.0.4",
    "@seedcss/seed-alert": "0.0.6",
    "@seedcss/seed-button": "0.0.6",
    "@seedcss/seed-color-scheme": "0.0.6",
    "@seedcss/seed-control": "0.0.6",
    "@seedcss/seed-dash": "0.0.6",
    "@seedcss/seed-family": "0.0.6",
    "@seedcss/seed-list": "0.0.6",
    "@seedcss/seed-this": "0.0.6",
    "compute-scroll-into-view": "^1.0.11",
    "focus-trap-react": "4.0.1",
    "highlight.js": "^9.12.0",
    "lodash.get": "^4.4.2",
    "path-to-regexp": "^2.4.0",
    "pluralize": "^7.0.0",
    "polished": "^2.3.0",
    "popper.js": "1.14.3",
    "prop-types": "^15.6.1",
    "react-sortable-hoc": "^0.6.7",
    "react-transition-group": "2.2.0",
    "unistore": "^3.1.0"
  },
  "devDependencies": {
    "@helpscout/artboard": "^0.1.9",
    "@helpscout/helix": "^0.0.13",
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-links": "^3.4.11",
    "@storybook/addon-options": "^3.4.11",
    "@storybook/addons": "^3.4.11",
    "@storybook/cli": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.3",
    "@types/lodash.get": "^4.4.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-transition-group": "^2.0.14",
    "@types/storybook__react": "^3.0.9",
    "autoprefixer": "7.1.1",
    "awesome-typescript-loader": "^4.0.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-emotion": "^9.2.11",
    "babel-plugin-inline-svg": "0.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "3.1.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-runtime": "6.23.0",
    "chalk": "1.1.3",
    "check-dependencies": "1.1.0",
    "coveralls": "3.0.1",
    "cpx": "^1.5.0",
    "cross-env": "5.1.0",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "enzyme": "2.9.1",
    "eslint": "4.19.1",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.9.1",
    "eslint-scope": "3.7.1",
    "flow-bin": "0.72.0",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.3",
    "hoek": "5.0.3",
    "husky": "1.0.0-rc.13",
    "istanbul-api": "1.2.2",
    "istanbul-reports": "1.1.4",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-watch-typeahead": "^0.2.0",
    "mkdirp": "0.5.1",
    "node-sass": "^4.9.2",
    "np": "^3.0.4",
    "nyc": "12.0.2",
    "object-assign": "4.1.1",
    "object-values": "2.0.0",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "postcss-scss": "1.0.2",
    "prettier": "1.12.1",
    "pretty-quick": "^1.4.1",
    "react": "15.6.1",
    "react-addons-test-utils": "15.6.2",
    "react-dev-utils": "^3.0.2",
    "react-dom": "15.6.1",
    "react-frame-component": "^1.1.0",
    "react-page-object": "1.1.0",
    "react-router": "4.2.0",
    "react-router-dom": "^4",
    "react-test-renderer": "15.6.1",
    "rgb-hex": "2.1.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "sass-pathfinder": "^0.0.5",
    "seed-harvester": "^0.1.5",
    "sinon": "^5.0.10",
    "slugify": "^1.3.1",
    "style-loader": "0.18.2",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.1",
    "webpack": "^3.12.0"
  },
  "nyc": {
    "exclude": [
      "config/**/*.js",
      "dist/**/*.js",
      "scripts/**/*.js",
      "stories/**/*.js"
    ],
    "cache": false
  }
}
